            "email":"nicolas.nkruger@gmail.com"
        }
    },
    "paths": {
        "/categories": {
            "post": {
                "tags": ["Category"],
                "sumary": "Create a category",
                "description": "Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "propertires": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "category name sample",
                                    "description": "Create a new category"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "category already exists"
                    }
                }
            },
            "get": {
                "tags": [
                    "Category"
                ],
                "sumary": "List categories",
                "description": "List all categories",
                "responses": {
                    "200": {
                        "description": "List",
                        "content": {
                            "schema":{
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string"
                                        },
                                        "description": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}